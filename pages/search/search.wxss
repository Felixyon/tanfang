/*收藏wxsscopy过来*/
image{
  height: 100%;
  width: 100%;
  overflow: hidden;
  border-radius: 5px;
}
label{
  float: left;
}
.content{
  width:80%;
  height: auto;
  line-height: 70rpx;
  font-size: 25rpx;
  margin-top:10rpx;
}
.list{
  border: 1rpx solid #2db07a;
  border-radius: 5px;
  width:80%;
  height: 120px;
  margin: 5px 0 5px 0;
  
}
.listcontent{
  width:100%;
  height:100%;
}
.list-img{
  width: 30%;
  height: 100%;
  float: left;
  overflow: hidden;
}
.list-introduce{
  float: left;
  width: 60%;
  height:80%;
  margin:3% 0 8% 0;
  
}
.list-introduce view{
  float: left;
  width: 3%;
  height:1%;
}
.list-introduce view label{
  width: 40%;
  height: 30px;
  margin:0 0 0 15px;
  position: absolute;
}
.author-info{
  clear: both;
  width: 50%;
  height:20px; 
  margin:30px 0 0 4%;
  font-size: 13px;
  position: absolute;
}

.state{
  width: 40%;
  height: 100%;
  margin:60px 0 0 4%;
  font-size: 14px;
  position: absolute;
}
.state-hot{
  width: 100%;
  display: inline-block;
  line-height: 4px;
  margin: 17px 0 0 0;
}
.calculate{
 width: 100%;
 margin: -8px 0 0 0;
}
.bookName{
  word-break:keep-all;
}